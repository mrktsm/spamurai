{
  "manifest_version": 3,
  "name": "Spamurai",
  "description": "In progress",
  "version": "1.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["scripting"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
